{
  "log": {
    "access": "/var/log/xray/access.log"
  },
  "inbounds": [
    {
      "listen": "127.0.0.1",
      "port": VMESSWSTLS_PORT,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "VMESSWSTLS_UUID",
            "flow": "xtls-rprx-direct"
# vmesswstls
          }
        ],
        "disableInsecureEncryption": false
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "acceptProxyProtocol": true,
          "path": "/vmesswstls"
        }
      }
    },
    {
      "listen": "PUBLIC_IP",
      "port": VMESSWS_PORT,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "VMESSWS_UUID",
            "flow": "xtls-rprx-direct"
# vmessws
          }
        ],
        "disableInsecureEncryption": false
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "path": "/vmessws"
        }
      }
    },
    {
      "port": VLESS_PORT,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "VLESS_UUID",
            "flow": "xtls-rprx-direct"
# vless
          }
        ],
        "decryption": "none",
        "fallbacks": [
          {
            "dest": 80
          },
          {
            "path": "/vmesswstls",
            "dest": VMESSWSTLS_PORT,
            "xver": 1
          },
          {
            "path": "/vlesswstls",
            "dest": VLESSWSTLS_PORT,
            "xver": 1
          },
          {
            "path": "/trojanwstls",
            "dest": TROJANWSTLS_PORT,
            "xver": 1
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "xtls",
        "xtlsSettings": {
          "alpn": ["http/1.1"],
          "certificates": [
            {
              "certificateFile": "/etc/letsencrypt/live/DOMAIN_NAME/fullchain.pem",
              "keyFile": "/etc/letsencrypt/live/DOMAIN_NAMEe/privkey.pem"
            }
          ]
        }
      }
    },
    {
      "listen": "127.0.0.1",
      "port": VLESSWSTLS_PORT,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "VLESSWSTLS_UUID",
            "flow": "xtls-rprx-direct"
# vlesswstls
          }
        ],
        "decryption": "none",
        "fallbacks": []
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "acceptProxyProtocol": true,
          "path": "/vlesswstls"
        }
      }
    },
    {
      "listen": "PUBLIC_IP",
      "port": VLESSWS_PORT,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "VLESSWS_UUID",
            "flow": "xtls-rprx-direct"
# vlessws
          }
        ],
        "decryption": "none",
        "fallbacks": []
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "path": "/vlessws"
        }
      }
    },
    {
      "listen": "127.0.0.1",
      "port": TROJANWSTLS_PORT,
      "protocol": "trojan",
      "settings": {
        "clients": [
          {
            "password": "TROJANWSTLS_UUID",
            "flow": "xtls-rprx-direct"
# trojanwstls
          }
        ],
        "fallbacks": []
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "acceptProxyProtocol": true,
          "path": "/trojanwstls"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom"
    }
  ]
}
